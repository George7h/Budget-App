<h1>Edit Category</h1>

<%= form_with(model: @group, url: group_path(@group), method: :patch) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.file_field :icon %>
  </div>

  <%= form.submit "Save" %>
<% end %>

<%= link_to "Back", groups_path %>
