<%= form_with(model: @group, url: group_path(@group), method: :patch) do |form| %>
<nav class="navbar navbar-expand-lg navcolor">
  <%= link_to groups_path do %>
    <%= image_tag "arrow_back.svg", class: "arrow-back" %>
  <% end %>
  <h2 id="headingcat">Edit Category</h2>
  <div class="actions">
    <%= form.submit "Save", class: 'registration-submission' %>
  </div>
</nav>
<div class="new_expense_form">
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.file_field :icon %>
  </div>
</div>
<% end %>

